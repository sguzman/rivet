{
  "name": "rivet-workspace",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.16.1",
  "scripts": {
    "ui:dev": "pnpm --dir crates/rivet-gui/ui run dev",
    "ui:build": "pnpm --dir crates/rivet-gui/ui run build",
    "ui:check": "pnpm --dir crates/rivet-gui/ui run check",
    "ui:lint": "pnpm --dir crates/rivet-gui/ui run lint",
    "ui:test": "pnpm --dir crates/rivet-gui/ui run test",
    "ui:e2e": "pnpm --dir crates/rivet-gui/ui run e2e",
    "tauri:dev": "cargo tauri dev --manifest-path crates/rivet-gui/src-tauri/Cargo.toml",
    "tauri:build": "cargo tauri build --manifest-path crates/rivet-gui/src-tauri/Cargo.toml"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0"
  }
}
